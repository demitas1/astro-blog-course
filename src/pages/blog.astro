---
import BaseLayout from "../layouts/BaseLayout.astro"

const allPosts = await Astro.glob("../pages/posts/*.md")
//console.log(allPosts.map((post) => post.frontmatter))

---

<BaseLayout pageTitle="blog">
  <ul>
    {
    allPosts.map((post) => (
    <li>
      <a href={`${post.url}`}>
        {post.frontmatter.title}
      </a>
    </li>
    ))
    }
  </ul>
</BaseLayout>
